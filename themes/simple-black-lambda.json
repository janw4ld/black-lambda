{
	"$schema": "vscode://schemas/color-theme",
	// "include": "./dark_plus.json",
	// "name": "Simple Black Lambda",
	"type": "dark",
	"colors": {
		"activityBar.background": "#000000",
		"activityBar.border": "#444444",
		"sideBar.background": "#000000",
		"sideBar.border": "#444444",
		"menu.background": "#000000",
		"menu.foreground": "#CCCCCC",
		"tab.activeBackground": "#000000",
		"tab.activeBorder": "#0076BF",
		"tab.border": "#000000",
		"tab.activeBorderTop": "#0076BF",
		"tab.inactiveBackground": "#44444494",
		"titleBar.activeBackground": "#000000",
		"editorGroupHeader.tabsBackground": "#000000",
		"statusBar.background": "#000000",
		"editor.background": "#000000",
		"editor.foreground": "#D4D4D4",
		"editor.inactiveSelectionBackground": "#3A3D41",
		"editorIndentGuide.background": "#404040",
		"editorIndentGuide.activeBackground": "#707070",
		"editor.selectionHighlightBackground": "#ADD6FF26",
		"list.dropBackground": "#383B3D",
		"activityBarBadge.background": "#007ACC",
		"sideBarTitle.foreground": "#BBBBBB",
		"input.placeholderForeground": "#A6A6A6",
		"settings.textInputBackground": "#292929",
		"settings.numberInputBackground": "#292929",
	},
	"tokenColors": [
		// Basic source code color
		{
			"scope": [
				"source",
			],
			"settings": {
				"foreground": "#FFFFFF",
			},
		},
		// Comments
		{
			"scope": [
				"comment",
				"punctuation.whitespace.comment",
				"punctuation.definition.comment",
			],
			"settings": {
				"foreground": "#7E6B5F",
				"fontStyle": "",
			},
		},
		// Punctuation
		{
			"scope": [
				"punctuation",
				"meta.brace",
				"meta.delimiter",
				"meta.bracket",
			],
			"settings": {
				"foreground": "#D4BE98",
			},
		},
		// Constants
		{
			"scope": [
				"keyword.operator",
				"meta.delimiter",
			],
			"settings": {
				"foreground": "#A3D349",
			},
		},
		{
			"scope": [
				"constant",
				"keyword.operator.infix.tight.at",
				"variable.other.member",
				"meta.record punctuation.brace",
				"keyword.operator.lambda",
				"keyword.operator.colon",
			],
			"settings": {
				"foreground": "#CCA143",
				"fontStyle": "",
			},
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language",
				"string.quoted",
				"punctuation.definition.string",
				"keyword.operator.prefix.minus",
			],
			"settings": {
				"foreground": "#CE9178",
			},
		},
		{
			"scope": [
				"constant.character.escape",
				"punctuation.quasi.element",
				"punctuation.definition.template-expression",
				"punctuation.section.embedded",
				"storage.type.format",
				"constant.other.placeholder",
				"variable.interpolation",
				"meta.quasi-quotation",
			],
			"settings": {
				"foreground": "#ccc370",
			},
		},
		// Keywords
		{
			"scope": [
				"keyword",
				"modifier",
			],
			"settings": {
				"foreground": "#CC524B",
			},
		},
		{
			"scope": [
				"keyword.other.debugger",
			],
			"settings": {
				"foreground": "#EA6279",
			},
		},
		{
			"scope": [
				"keyword.operator.double-colon",
				"keyword.operator.arrow",
				"keyword.operator.big-arrow",
				"keyword.operator.period",
				"keyword.operator.eq",
				"keyword.operator.pipe",
				"keyword.operator.double-dot",
				"keyword.operator.lambda",
				"keyword.operator.prefix",
				"punctuation.backtick",
			],
			"settings": {
				"foreground": "#CC524B",
				"fontStyle": "bold",
			},
		},
		{
			"scope": [
				"keyword.operator.prefix.at",
			],
			"settings": {
				"foreground": "#B3DDF5",
				"fontStyle": "",
			},
		},
		{
			"scope": [
				"keyword.operator.prefix.hash",
				"entity.name.label",
				"constant.other.label",
			],
			"settings": {
				"foreground": "#8BBBC2",
				"fontStyle": "",
			},
		},
		{
			"scope": [
				"keyword.operator.postfix.hash",
				"keyword.operator.hash",
			],
			"settings": {
				"foreground": "#9099A7",
				"fontStyle": "",
			},
		},
		{
			"scope": [
				"keyword.operator.period",
			],
			"settings": {
				"foreground": "#CC524B",
				"fontStyle": "bold italic",
			},
		},
		{
			"scope": [
				"keyword.other.forall",
			],
			"settings": {
				"foreground": "#CC524B",
				"fontStyle": "italic",
			},
		},
		// Invalid code
		{
			"scope": [
				"invalid",
				"token.error-token",
			],
			"settings": {
				"foreground": "#AA2A2A",
				"fontStyle": "",
			},
		},
		// Types
		{
			"scope": [
				"storage.type",
				"support.type",
				"entity.name.type",
				"keyword.type",
			],
			"settings": {
				"foreground": "#6B95C5",
			},
		},
		// Variables
		{
			"scope": [
				"variable",
				"support.variable",
				"meta.definition.variable",
			],
			"settings": {
				"foreground": "#82AED8",
				// "fontStyle": "italic",
			},
		},
		// Namespacing
		{
			"scope": [
				"entity.name",
				"entity.other.attribute-name",
				"meta.function.type-declaration",
				"meta.other.unknown",
				"punctuation.backtick",
				"punctuation.decorator",
				"keyword.operator.promotion",
				"keyword.operator.function.infix",
			],
			"settings": {
				"foreground": "#96B946",
			},
		},
		{
			"scope": [
				"meta.import",
				"entity.name.namespace",
				"meta.declaration.module",
				"support.module",
				"support.node",
				"support.other.module",
				"support.type.object.module",
				"entity.name.type.module",
				"entity.name.type.class.module",
				"keyword.control.module",
			],
			"settings": {
				"foreground": "#73A36C",
			},
		},
		// Functions
		{
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function",
				"meta.function-call",
				"entity.name.function",
				"meta.definition.method",
			],
			"settings": {
				"foreground": "#66a89d",
			},
		},
		// Preprocessor
		{
			"scope": [
				"meta.preprocessor",
				"keyword.other.preprocessor",
				"punctuation.definition.preprocessor",
				"entity.name.function.preprocessor",
			],
			"settings": {
				"foreground": "#9B84DA",
			},
		},
		{
			"scope": [
				"keyword.other.preprocessor.extension",
			],
			"settings": {
				"fontStyle": "",
			},
		},
		{
			"scope": [
				"keyword.other.preprocessor",
			],
			"settings": {
				"fontStyle": "bold",
			},
		},
		// Classes (OO)
		{
			"scope": [
				"entity.name.type.class, support.class, entity.name.class, entity.other.inherited-class, storage.class",
			],
			"settings": {
				"foreground": "#7DAEA3",
			},
		},
		// Markdown, LaTeX and other formatting (taken from Gruvbox Material)
		{
			"name": "Markdown heading1",
			"scope": [
				"heading.1.markdown, markup.heading.setext.1.markdown",
